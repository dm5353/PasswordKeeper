{
  "manifest_version": 3,
  "name": "Password keeper",
  "version": "4.5",
  "action": {
    "default_popup": "main.html",
    "default_icon": "icon.png"
  },
  "permissions": ["storage", "activeTab"],
  "host_permissions": [
    "https://raw.githubusercontent.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["sticker.js"],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["sticker.html", "stickerStyle.css"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ]
}
